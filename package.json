  "main": "app/background.js",
  "build": {
    "appId": "com.ngrev",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "build": "gulp build",
    "prerelease": "gulp build --env=production",
    "release": "build && cp dist/mac/ngrev-0.0.0.dmg releases",
    "start": "gulp start",
    "pretest": "gulp build-unit --env=test",
    "test": "electron-mocha app/specs.js.autogenerated --renderer --require source-map-support/register",
    "coverage": "npm test -- -R scripts/istanbul-reporter",
    "pree2e": "gulp build-e2e --env=test",
    "e2e": "mocha app/e2e.js.autogenerated --require source-map-support/register"
  },
  "dependencies": {
    "@angular/common": "^4.0.0-rc.2",
    "@angular/compiler": "^4.0.0-rc.1",
    "@angular/compiler-cli": "^4.0.0-rc.1",
    "@angular/core": "^4.0.0-rc.1",
    "@angular/platform-browser": "^4.0.0-rc.2",
    "@angular/platform-browser-dynamic": "^4.0.0-rc.2",
    "@types/vis": "^4.17.3",
    "fs-jetpack": "^0.10.2",
    "ngast": "^0.0.14",
    "open": "0.0.5",
    "reflect-metadata": "^0.1.10",
    "run-sequence": "^1.2.2",
    "rxjs": "^5.2.0",
    "typescript": "^2.2.1",
    "vis": "^4.18.1",
    "zone.js": "^0.7.7"
  },
  "devDependencies": {
    "@types/electron": "^1.4.33",
    "@types/node": "^7.0.7",
    "@types/open": "0.0.29",
    "chai": "^3.5.0",
    "electron": "^1.4.7",
    "electron-builder": "^8.6.0",
    "electron-mocha": "^3.0.0",
    "gulp": "^3.9.0",
    "gulp-batch": "^1.0.5",
    "gulp-less": "^3.0.3",
    "gulp-plumber": "^1.1.0",
    "gulp-typescript": "^3.1.5",
    "gulp-util": "^3.0.6",
    "gulp-watch": "^4.3.5",
    "istanbul": "^0.4.3",
    "minimist": "^1.2.0",
    "mocha": "^3.0.2",
    "rollup": "^0.36.3",
    "rollup-plugin-istanbul": "^1.1.0",
    "source-map-support": "^0.4.2",
    "spectron": "^3.3.0"
  }
